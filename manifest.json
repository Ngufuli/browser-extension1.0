{
  "name": "Page Redirector",
  "description": "Redirects to other webpages or websites",
  "version": "1.0",
  "manifest_version": 2,

  "background": {
    "scripts": ["intotheweb.js"]
  },
  "permissions": [
    "webRequest",
    "*://facebook.com/*",
    "*://www.facebook.com/*",
    "*://yahoo.com/*",
    "*://search.yahoo.com/*",
    "*://google.com/*",
    "*://bing.com/*",
    "*://duckduckgo.com/*",
    "webRequestBlocking"
  ]
}
